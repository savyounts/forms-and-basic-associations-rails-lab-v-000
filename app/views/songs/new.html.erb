<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.collection_select :genre, Genre.all, :name, :name %>
  <%= f.label :notes %>
  <input name="song[note_ids][]" id="song_notes_1">
  <input name="song[note_ids][]" id="song_notes_2">
  <br />

  <%= f.submit %>
<% end %>
